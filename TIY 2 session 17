#include <stdio.h>

int main()
{
    int num[20];
    int i, n, avg, sum=0;
    
    printf("Amount of number u want to put in: ");
    scanf("%d",&n);
    
    
    for(i=0;i<n;i++){
    printf("Number %d: ",i+1);
    scanf("%d",&num[i]);
    sum+=num[i];
    }
    avg = sum/n;
    printf("Average: %d",avg);
